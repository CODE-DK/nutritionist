      "bundleIdentifier": "com.dietitian.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.dietitian.app"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
